<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Selenium E2E tests" parallel="classes" thread-count="4">
    <listeners>
        <listener class-name="com.realworld.seleniumrealworldapp.base.RetryListener" />
    </listeners>
    <test name="Selenium E2E tests">
        <classes>
            <class name="com.realworld.seleniumrealworldapp.ArticleDetailTests" />
            <class name="com.realworld.seleniumrealworldapp.AuthorDetailTests" />
            <class name="com.realworld.seleniumrealworldapp.GlobalFeedTests" />
            <class name="com.realworld.seleniumrealworldapp.LogoutTests" />
            <class name="com.realworld.seleniumrealworldapp.NewArticleTests" />
            <class name="com.realworld.seleniumrealworldapp.SignUpTests" />
            <class name="com.realworld.seleniumrealworldapp.TagsTests" />
            <class name="com.realworld.seleniumrealworldapp.UserDetailsTests" />
            <class name="com.realworld.seleniumrealworldapp.UserSettingsTests" />
        </classes>
    </test>
</suite>